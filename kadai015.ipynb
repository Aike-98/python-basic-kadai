      "source": [
        "class Human:\n",
        "  def __init__(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def printinfo(self):\n",
        "    print(f\"名前は{self.name}、年齢は{self.age}歳です。\")\n",
        "\n",
        "hanako = Human(\"花子\", 30)\n",
        "ichiro = Human(\"一郎\", 45)\n",
        "\n",
        "hanako.printinfo()\n",
        "ichiro.printinfo()"
      ]
